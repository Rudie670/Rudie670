import time
 
def fetch(url):
    time.sleep(2)  # 请求间隔2秒
    response = requests.get(url)
    return response
import time
from random import uniform
 
def fetch(url):
    time.sleep(uniform(1, 3))  # 随机延迟1到3秒
    response = requests.get(url)
    return response
import time
from random import uniform
 
def fetch(url):
    time.sleep(uniform(1, 3))  # 随机延迟1到3秒
    response = requests.get(url)
    return response
